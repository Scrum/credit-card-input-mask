{
  "name": "restricted-input",
  "version": "2.1.0",
  "description": "Restrict inputs to certain valid characters (e.g. formatting phone or card numbers)",
  "author": "Braintree <code@getbraintree.com> (https://www.braintreepayments.com/)",
  "license": "MIT",
  "main": "main.js",
  "homepage": "https://github.com/braintree/restricted-input",
  "repository": {
    "type": "git",
    "url": "git://github.com/braintree/restricted-input.git"
  },
  "bugs": "https://github.com/braintree/restricted-input/issues",
  "keywords": [
    "input",
    "formatting",
    "filtering",
    "text",
    "spacing",
    "restrict"
  ],
  "scripts": {
    "build": "mkdir -p dist; browserify ./main.js -o dist/restricted-input.js -s RestrictedInput -v",
    "doc": "jsdoc -r -R README.md -d jsdoc lib/",
    "doc:watch": "npm run doc && chokidar 'lib/**/*.js' -c 'npm run doc'",
    "prestart": "npm run build",
    "start": "node ./test/support/server.js",
    "development": "npm start & chokidar 'lib/**/*.js' -c 'npm run build; echo $(tput setaf 2)rebuilt$(tput sgr0)'",
    "lint": "eslint lib test",
    "test": "npm run lint && npm run test:unit",
    "test:watch": "karma start config/karma.js",
    "test:unit": "karma start config/karma.js --single-run",
    "test:integration": "wdio wdio.conf.js",
    "test:all": "npm run test && npm run test:integration",
    "publish:demo": "./publish-gh-pages.sh",
    "prepublish": "npm run build",
    "postpublish": "npm run publish:demo"
  },
  "dependencies": {
    "@braintree/browser-detection": "^1.9.0"
  },
  "devDependencies": {
    "@wdio/browserstack-service": "^6.1.4",
    "@wdio/cli": "^6.1.5",
    "@wdio/local-runner": "^6.1.6",
    "@wdio/mocha-framework": "^6.1.6",
    "@wdio/spec-reporter": "^6.1.5",
    "@wdio/sync": "^6.1.5",
    "browserify": "^16.5.0",
    "browserstack-local": "^1.4.5",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-braintree": "^4.0.0",
    "express": "^4.17.1",
    "jsdoc": "^3.6.4",
    "karma": "^5.0.4",
    "karma-browserify": "^7.0.0",
    "karma-chai-sinon": "0.1.5",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "lodash.assign": "^4.2.0",
    "mocha": "^7.1.2",
    "phantomjs-prebuilt": "^2.1.16",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "uuid": "^8.0.0",
    "watchify": "^3.7.0",
    "webdriverio": "^6.1.5"
  }
}
